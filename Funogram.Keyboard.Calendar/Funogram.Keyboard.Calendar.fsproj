<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>1.3.0</Version>
    <Authors>Ivan Marinin</Authors>
    <Description>Calendar inline keyboard for Funogram</Description>
    <PackageProjectUrl>https://github.com/dohly/funogram.keyboard</PackageProjectUrl>
    <PackageLicenseUrl>https://licenses.nuget.org/MIT</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/dohly/funogram.keyboard</RepositoryUrl>
    <AssemblyVersion>1.3.0.0</AssemblyVersion>
  </PropertyGroup>

  <ItemGroup>
    <Compile Include="Calendar.fs" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Funogram.Keyboard" Version="1.3.0" />
  </ItemGroup>

</Project>
